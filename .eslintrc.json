{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "commonjs"
    },
    "extends": ["eslint:recommended", "prettier"],
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        "no-console": "error",
        "arrow-body-style": 0,
        "operator-linebreak": 0,
        "comma-dangle": 0
    }
}
